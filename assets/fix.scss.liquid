  -webkit-transition: 0.3s;
  -webkit-transition-delay: 0.3s;
  -webkit-transition: 0.3s 0.3s;
  transition: 0.3s 0.3s;
  &:after {
    content: '';
    position: absolute;
    height: 22px;
    width: 1px;
    background: #fff;
    top: -10px;
  }
}

.float-btn-group {
  position: relative;
  float: right;
  -webkit-transition: 0.3s;
  transition: 0.3s;
  .btn-triger {
    z-index: 15;
    float: left;
    padding: 12px;
    padding-top: 6px;
    background-color: #228b22;
  }
}

.icon-bars {
  display: none;
}

.float-btn-group {
  .btn-list {
    position: absolute;
    right: 0;
    -webkit-transition: 0.3s;
    transition: 0.3s;
    li {
      display: inline-block;
    }
  }
  &.open {
    .icon-bars {
      display: inline-block;
      -webkit-transform: rotate(45deg);
      transform: rotate(45deg);
    }
    .btn-triger svg {
      display: none;
    }
  }
  .btn-triger {
    display: none;
  }
  &.open .btn-triger {
    display: inline-block;
    background: #e91e63;
    padding: 0;
  }
}

.dark .float-btn-group.open .btn-triger {
